@{
    Layout = null;
}
@*<link href="~/Content/bootstrap-theme.css" rel="stylesheet" />*@
<h2>Export Excel File For SMS</h2>
<hr />
<div ng-controller="ExportExcelForSMSController" ng-init="LoadAllDropDown()">
    <form class="form-horizontal" ng-submit="" name="export_SMS_form">
        <div class="form-group">
            <div class="col-md-2">
                <label>File Path</label>
            </div>
            <div class="col-md-4">
                <input type="text" class="form-control input-sm" readonly data-ng-model="ExportExcelforSMSPath" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-2">
                <label>OCode</label>
            </div>
            <div class="col-md-4">
                <input type="text" class="form-control input-sm"  data-ng-model="dtOcede" required/>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-2">
                <label>File name</label>
            </div>
            <div class="col-md-4">
                <input type="text" class="form-control input-sm"  data-ng-model="dtFilename" required />
            </div>
        </div>     

        <div class="form-group">
            <div class="col-md-1 col-md-offset-2">
                <input type="submit" value="Export" ng-click="ExportExcelFile()" class="btn btn-default" @*style="margin-left:-16px"*@ />
                @*<br /><br /><a href=".../C:/BULK_SMS/{{downloadFilename}}" download="{{downloadFilename}}">Download</a>*@
       
            </div>
            <div class="col-md-1 col-md-offset-2">         
                
            </div>
        </div>
    </form>
</div>